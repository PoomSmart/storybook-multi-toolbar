{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "esnext",
    ],
    "noImplicitAny": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "jsx": "react",
    "declaration": true,
    "paths": {
      "./registerComponent": [
        "./registerComponent.tsx"
      ]
    }
  }
}